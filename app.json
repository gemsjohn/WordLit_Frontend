{
  "expo": {
    "name": "WordLit",
    "slug": "WordLit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/WL_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#654785"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.WordLit",
      "versionCode": 1,
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/WL_Logo.png",
        "backgroundColor": "#654785"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4dccd2e5-5ae6-43cc-b0c8-67504e483a23"
      }
    }
  }
}
